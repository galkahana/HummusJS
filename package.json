{
  "name": "hummus",
  "version": "1.0.55",
  "description": "Create, read and modify PDF files and streams",
  "homepage": "http://pdfhummus.com/",
  "license": "Apache-2.0",
  "author": "Gal Kahana <gal.kahana@hotmail.com>",
  "main": "./hummus.js",
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "mocha -R tap ./tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/galkahana/HummusJS.git"
  },
  "keywords": [
    "pdf",
    "pdfhummus"
  ],
  "files": [
    "src",
    "hummus.js",
    "binding.gyp"
  ],
  "dependencies"  : {
      "node-pre-gyp": "^0.6.13"
   },
   "bundledDependencies":["node-pre-gyp"],
   "devDependencies": {
      "aws-sdk": "~2.0.0-rc.15",
      "chai": "^3.4.0",
      "mocha": "^2.3.3"
  },
  "binary": {
      "module_name": "hummus",
      "module_path": "./binding",
      "remote_path": "./{module_name}/v{version}/{toolset}/",
      "host": "https://hummus.s3-us-west-2.amazonaws.com",
      "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
    },
   "publishConfig": {
      "registry": "https://registry.npmjs.org/"
   }  
}
